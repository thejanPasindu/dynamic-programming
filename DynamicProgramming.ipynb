   "source": [
    "def fibonacci(n, memo = {}):\n",
    "    if n in memo: return memo[n]\n",
    "    if n == 1: return 1\n",
    "    if n == 0: return 0\n",
    "    memo[n] = fibonacci(n-1, memo) + fibonacci(n-2, memo)\n",
    "    \n",
    "print(fibonacci(5))"
   ]
